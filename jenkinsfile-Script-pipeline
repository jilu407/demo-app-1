node {
	stage('Checkout code') {
         git url: 'https://github.com/jilu407/jenkins-script-pipeline.git'
           
    }
	stage('Build'){
		echo "build pass"
		
	}
	stage('test'){
		echo 'test pass'
	}
	stage('Compile'){
		echo 'Compile pass'
	}
	stage('package'){
		echo 'package' 
	}
	stage('Ansible Deploy'){
		echo "Deploying the Project.........."
	}
}
